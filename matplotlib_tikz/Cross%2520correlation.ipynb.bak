     "input": [
      "# Calculate lag position of maximal correlation\n",
      "def lag_ix(x,y):\n",
      "\n",
      "    corr = np.correlate(x,y,mode='full')\n",
      "    pos_ix = np.argmax( np.abs(corr) )\n",
      "    lag_ix = pos_ix - (corr.size-1)/2\n",
      "    return lag_ix\n",
      "\n",
      "lag_ix(A,B)"
     ],
